{
  "name": "aakashgangaa-web-app",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "clean:i": "rm -rf node_modules/ && npm i",
    "start": "npm run build && babel-node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec babel-node src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Aditya Karnam",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.3.1",
    "cors": "2.8.5",
    "express": "4.16.3",
    "react": "16.11.0",
    "react-animated-text": "^0.1.4",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "16.11.0",
    "react-icons": "^3.10.0",
    "reakit": "1.0.0-beta.11"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/node": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.0",
    "babel-loader": "8.0.0",
    "clean-webpack-plugin": "1.0.0",
    "concurrently": "4.0.0",
    "css-loader": "2.0.0",
    "eslint": "5.0.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "file-loader": "3.0.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "^6.0.0",
    "nodemon": "1.17.3",
    "recompose": "0.30.0",
    "style-loader": "0.23.0",
    "url-loader": "1.0.1",
    "webpack": "4.5.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.3"
  }
}
